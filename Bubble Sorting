#include <stdio.h>
int main(void){
     int nums[10]={200, 500, 200, 34,4300, 5200,45,1600,10,21}, temp, size=10;
     printf("values in array before swapping\n");
     for(int i=0;i<size;i++){
            printf("%d ",nums[i]);}
     printf("\n");
            for (int i=0;i<size-1;i++){
                for(int j=0; j<size-1-1; j++){
                        if(nums[j]>nums [j+1]){
                            temp=nums[j];
                nums[j]=nums[j+1];
                nums[j+1]=temp;

}
}
}
printf("Values in array after sorting\n");
for(int i=0;i<size; i++) {
        printf("%d ",nums[i]); }

return 0;
}
